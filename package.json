{
  "name": "@hashicorp/ember-cli-api-double",
  "version": "4.0.0",
  "description": "An ember-cli wrapper around @hashicorp/api-double",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/hashicorp/ember-cli-api-double",
  "license": "MPL-2.0",
  "author": "",
  "files": [
    "addon/*",
    "app/!(.gitkeep)",
    "config/!(ember-try.js)",
    "index.js"
  ],
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@hashicorp/api-double": "^1.6.1",
    "array-range": "^1.0.1",
    "broccoli-file-creator": "^2.1.1",
    "broccoli-merge-trees": "^4.2.0",
    "ember-auto-import": "^2.4.2",
    "ember-cli-babel": "^8.2.0",
    "merge-options": "^3.0.4",
    "pretender": "^3.4.7",
    "recursive-readdir-sync": "^1.0.6"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.3.0",
    "@ember/test-helpers": "^5.4.1",
    "@embroider/test-setup": "^4.0.0",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.0.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "^6.8.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-htmlbars": "^7.0.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^3.0.1",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.1.1",
    "ember-source": "~6.9.0",
    "ember-template-lint": "^7.9.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-ember": "^12.7.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-qunit": "^8.2.5",
    "loader.js": "^4.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "qunit": "^2.24.2",
    "webpack": "^5.74.0"
  },
  "engines": {
    "node": "14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "overrides": {
    "braces": "^3.0.3",
    "ansi-html": "^0.0.8"
  }
}
